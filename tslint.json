{
  "rules": {
    "curly": true,
    "forin": true,
    "label-position": true,
    "switch-default": true,
    "radix": true,
    "triple-equals": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [ true, "debug", "info", "time", "timeEnd", "trace"],
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-conditional-assignment": true,
    "no-switch-case-fall-through": true,
    "no-construct": true,
    "no-null-keyword": false,
    "no-string-literal": true,
    "no-shadowed-variable": true,
    "no-string-throw": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "eofline": true,
    "indent": [true, "spaces"],
    "cyclomatic-complexity": [true, 20],
    "linebreak-style": [false, "LF"],
    "max-line-length": [false, 180],
    "no-default-export": true,
    "no-require-imports": true,
    "no-trailing-whitespace": true,
    "prefer-const": true,
    "trailing-comma": [true, { "singleline": "never", "multiline": "always" }],
    "align": [true, "parameters", "arguments", "statements"],
    "arrow-parens": [true],
    "class-name": true,
    "comment-format": [ true, "check-space"],
    "import-spacing": true,
    "interface-name": [true, "always-prefix"],
    "interface-over-type-literal": false,
    "new-parens": true,
    "no-consecutive-blank-lines": true,
    "no-parameter-properties": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "object-literal-shorthand": true,
    "one-variable-per-declaration": true,
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "always"],
    "space-before-function-paren": true,
    "whitespace": [ true, "check-branch", "check-decl", "check-operator", "check-module", "check-separator", "check-type", "check-typecast"],
    "variable-name": [false, "ban-keywords", "check-format"],
    "member-ordering": [true, "public-before-private", "static-before-instance", "variables-before-functions"],
    "no-any": false,
    "no-internal-module": true,
    "no-var-requires": false,
    "typedef": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "typedef-whitespace": [
      true, {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ]
  }
}
